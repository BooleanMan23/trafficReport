<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/tulisanNamaJalan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nama Jalan : " />

    <TextView
        android:id="@+id/detailNamaJalanTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nama Jalan"
        android:textSize="24sp" />
    <TextView
        android:id="@+id/tulisanDeskripsiJalan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Deskripsi : " />

    <TextView
        android:id="@+id/detailDeskripsiJalanTextView"
        android:layout_width="match_parent"
        android:layout_height="134dp"
        android:text="Deskripsi"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/dd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Lokasi Dari Map " />

    <TextView
        android:id="@+id/wolo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:text="LOKASI MAP"
        />

    <TextView
        android:id="@+id/tulisanTanggal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="tanggal dibuat " />

    <TextView
        android:id="@+id/detailTanggalDibuatTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:text="tanggal dibuat"
       />









</LinearLayout>